[
    {
        "clean": ["~"]
    },
    {
        "link": {
            "~/.aliases": "aliases",
            "~/.exports": "exports",
            "~/.functions": "functions",
            "~/.gitconfig": "gitconfig",
            "~/.gdbinit": "gdbinit",
            "~/.gitignore_global": "gitignore_global",
            "~/.oh-my-zsh": "oh-my-zsh/",
            "~/.zshrc": "zshrc"
        }
    },
    {
        "shell": [
            ["git submodule update --init --recursive", "Installing submodules"]
        ]
    }
]
